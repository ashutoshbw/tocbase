{
  "type": "module",
  "name": "tocbase",
  "version": "3.1.3",
  "description": "A JavaScript library for quickly creating a table of contents inside the browser.",
  "repository": {
    "type": "git",
    "url": "https://github.com/ashutoshbw/tocbase.git"
  },
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist/*",
    "build": "rollup -c",
    "build-doc": "pandoc -s -t html --standalone --template=./docs/template.html ./docs/doc.md -o ./docs/index.html",
    "dev": "rollup -c -w",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "tocbase",
    "table of contents",
    "toc",
    "structure",
    "generate",
    "html",
    "vanilla",
    "plugin",
    "table",
    "of",
    "contents"
  ],
  "author": {
    "name": "Ashutosh Biswas",
    "email": "ashutoshbw314@gmail.com",
    "url": "https://ashutoshbiswas.dev"
  },
  "license": "MIT",
  "bugs": "https://github.com/ashutoshbw/tocbase/issues",
  "devDependencies": {
    "rollup": "^2.78.0",
    "rollup-plugin-terser": "^7.0.2"
  }
}
